{
  "version": "0.2.0",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/production/index.js",
  "module": "dist/esm/production/index.js",
  "exports": {
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dev": {
      "production": {
        "require": "./dist/cjs/production/index.js",
        "import": "./dist/esm/production/index.js"
      },
      "require": "./dist/cjs/development/index.js",
      "import": "./dist/esm/development/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./esm": {
      "development": "./dist/esm/development/index.js",
      "production": "./dist/esm/production/index.js",
      "default": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./cjs": {
      "development": "./dist/cjs/development/index.js",
      "production": "./dist/cjs/production/index.js",
      "default": "./dist/cjs/production/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "name": "solid-uppy",
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@uppy/core": "^2.1.5",
    "@uppy/dashboard": "^2.1.4",
    "@uppy/drag-drop": "^2.0.6",
    "@uppy/drop-target": "^1.1.1",
    "@uppy/file-input": "^2.0.5",
    "@uppy/informer": "^2.0.5",
    "@uppy/status-bar": "^2.1.2",
    "eslint": "^8.9.0",
    "eslint-config-lxsmnsyc": "^0.4.0",
    "pridepack": "^1.0.4",
    "solid-js": "^1.3.7",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@uppy/core": "^2.0",
    "@uppy/dashboard": "^2.0",
    "@uppy/drag-drop": "^2.0",
    "@uppy/drop-target": "^1.1",
    "@uppy/file-input": "^2.0",
    "@uppy/informer": "^2.0",
    "@uppy/status-bar": "^2.0",
    "solid-js": "^1.2"
  },
  "scripts": {
    "prepublish": "pridepack clean && pridepack build",
    "build": "pridepack build",
